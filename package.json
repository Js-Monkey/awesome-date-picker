{
  "name": "bettter-datepicker",
  "version": "0.0.2",
  "description": "A date-picker",
  "main": "dist/betterDatePicker.umd.js",
  "module": "dist/betterDatePicker.es5.js",
  "scripts": {
    "build:css": "gulp build --gulpfile build/gulpfile.js",
    "dev": "webpack-dev-server --config build/webpack.demo.config.js",
    "prebp": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c build/rollup.config.js && npm run build:css",
    "build:demo": "webpack --config build/webpack.demo.config.js",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "lint": "eslint src --fix --ext .ts,.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "./build/jest.config"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint",
      "git add"
    ]
  },
  "devDependencies": {
